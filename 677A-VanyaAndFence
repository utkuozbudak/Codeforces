#include <iostream>
using namespace std;

int main(int argc, const char * argv[])
{
    int n,l;
    cin >> n >> l;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int res = 0;
    for(int i=0;i<n;i++)
    {
        if(arr[i] > l) res += 2;
        else res += 1;
    }
    cout<<res;
    return 0;
}
