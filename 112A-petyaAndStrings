#include <iostream>
#include "string"
using namespace std;

int main(int argc, const char * argv[]) {
    string str1,str2;
    cin >> str1 >> str2;
    int res1 = 0;
    int res2 = 0;
    
    for(int i=0; i<str1.length();i++){
        res1 += tolower(str1[i]);
        res2 += tolower(str2[i]);
    }
    if(res1 > res2) cout << "1";
    if(res1 < res2) cout << "-1";
    else cout << "0";
    
    return 0;
}
